#include <stdio.h>
#include <math.h>
int main()
    {
int n,k,j,p,c,h,s;
printf("What is the number you want prime factored?");
scanf("%d",&n);
j=2;
while (j<=n)
    {
if (j%2!=0)
    {
  for(c=2;c<=j-1;c++)
    {
    h=j%c;
    if (h==0)
        {
            break;
        }


    }
    if ((c==j)&&(n%j==0))
    {
        printf("%d\n",j);
    }
}
j=j+1;
    }

    return 0;
    }
